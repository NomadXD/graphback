{
  "extends": [
    "config:base",
    ":pinAllExceptPeerDependencies"
  ],
  "groupName": "all",
  "ignoreDeps": [
    "chalk"
  ],
  "ignorePaths": [
    "**/node_modules/**",
    "**/bower_components/**",
    "**/vendor/**",
    "**/__tests__/**",
    "**/test/**",
    "**/tests/**"
  ],
  "packageRules": [
    {
      "packageNames": [
        "graphql"
      ],
      "versionStrategy": "widen"
    }
  ]
}